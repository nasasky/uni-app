
view {
	display: block;
}
.content {
	width: 660rpx;
	margin: 0 auto;
}
.page {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	width: 100%;
	text-align: center;
}
.result-list {
	padding: 40rpx;
}
.result-list view {
	padding: 10rpx 0;
}
.result-list view rich-text {
	font-size: 28rpx;
}
.btn-list ._button {
	width: 100%;
	height: 80rpx;
	margin: 0 auto 40rpx;
	text-align: center;
	line-height: 80rpx;
	background: #918984;
	color: #ffffff;
	border-radius: 40rpx;
}
.address-name {
	font-size: 28rpx;
}

